{
  "packages": [
    "application",
    "auth",
    "cart",
    "checkout",
    "content",
    "core",
    "experience",
    "form",
    "i18n",
    "injector",
    "presets",
    "product",
    "search",
    "site",
    "themes",
    "ui",
    "user",
    "utilities"
  ],
  "version": "1.0.0-alpha.0",
  "npmClient": "npm",
  "command": {
    "publish": {
      "conventionalCommits": true,
      "forcePublish": true,
      "contents": "dist",
