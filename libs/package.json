{
  "name": "libs",
  "version": "0.0.0",
  "description": "",
  "private": true,
  "author": "",
  "scripts": {
    "prerelease": "npm run pack",
    "release": "lerna publish --yes",
    "release-from-tag": "npm run release -- --conventional-graduate",
    "release-tag": "npm run release -- --conventional-prerelease --dist-tag $RELEASE_TAG --preid $RELEASE_TAG",
    "release-branch": "node ../tools/release/release.js",
    "pack": "run-s pack:*",
    "prepack:libs": "npm run clean:dist",
    "pack:libs": "lerna exec --concurrency 1 --stream -- node \\$LERNA_ROOT_PATH/../tools/release/pack-lib.js",
    "postversion": "lerna exec --concurrency 1 --stream -- node \\$LERNA_ROOT_PATH/../tools/release/update-lib.js",
    "clean:dist": "rm -rf */dist"
  }
}
