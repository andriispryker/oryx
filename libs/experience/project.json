{
  "root": "libs/experience",
  "sourceRoot": "libs/experience/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/web:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/experience",
        "entryFile": "libs/experience/src/index.ts",
        "tsConfig": "libs/experience/tsconfig.lib.json",
        "project": "libs/experience/package.json",
        "assets": [
          {
            "glob": "*",
            "input": "fes/libs/experience/*.md",
            "output": "fes/dist/libs/experience"
          }
        ],
        "format": ["esm", "umd", "cjs"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/experience/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": ["tsc --project tsconfig.spec.json", "vitest run"],
        "parallel": false,
        "cwd": "libs/experience"
      }
    },
    "test-watch": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": ["tsc --project tsconfig.spec.json", "vitest"],
        "parallel": false,
        "cwd": "libs/experience"
      }
    }
  },
  "tags": []
}
