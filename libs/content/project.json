{
  "projectType": "library",
  "root": "libs/content",
  "sourceRoot": "libs/content",
  "prefix": "fes",
  "targets": {
    "build": {
      "executor": "./tools/executors/components-library:build"
    },
    "paths": {
      "executor": "./tools/executors/components-library:paths"
    },
    "test": {
      "executor": "./tools/executors/oryx-vite:vitest"
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/content/**/*.ts"]
      },
      "outputs": ["{options.outputFile}"]
    },
    "storybook": {
      "executor": "@nrwl/storybook:storybook",
      "options": {
        "uiFramework": "@storybook/web-components",
        "port": 4400,
        "config": {
          "configFolder": "libs/content/.storybook"
        },
        "staticDir": ["libs/content/public"]
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "stylelint": {
      "executor": "nx-stylelint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/content/**/*.css",
          "libs/content/**/*.styles.ts"
        ]
      }
    }
  },
  "tags": []
}
