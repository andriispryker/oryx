{
  "root": "apps/storefront",
  "projectType": "application",
  "sourceRoot": "apps/storefront/src",
  "tags": [],
  "targets": {
    "ssr": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "npm run dev:ssr",
        "cwd": "apps/storefront"
      }
    },
    "serve": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "npm run dev",
        "cwd": "apps/storefront"
      }
    },
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "npm run build",
        "cwd": "apps/storefront"
      }
    },
    "test": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "vitest",
        "config": "apps/storefront/vitest.config.ts"
      }
    },
    "test-watch": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "vitest watch",
        "config": "apps/storefront/vitest.config.ts"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/storefront/**/*.ts"]
      }
    }
  }
}
