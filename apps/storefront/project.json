{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/storefront/src",
  "tags": [],
  "targets": {
    "ssr": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "npm run dev:ssr",
        "cwd": "apps/storefront"
      }
    },
    "start-ssr-prod": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "NODE_ENV=production npm run dev:ssr",
        "cwd": "apps/storefront"
      }
    },
    "serve": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "npm run dev",
        "cwd": "apps/storefront"
      }
    },
    "serve-testing": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "npm run dev -- --mode testing",
        "cwd": "apps/storefront"
      }
    },
    "build-client": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "npm run build",
        "cwd": "apps/storefront"
      }
    },
    "build-ssr": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "npm run build:ssr",
        "cwd": "apps/storefront"
      }
    },
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": ["nx build-ssr storefront"],
        "parallel": false
      }
    },
    "build-testing": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "npm run build -- --mode testing",
        "cwd": "apps/storefront"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/storefront/**/*.ts"]
      }
    },
    "generate-dep-tree": {
      "executor": "./tools/executors/generate-dependency-tree:generate-dep-tree",
      "options": {
        "project": "storefront"
      }
    },
    "check-dep-tree": {
      "executor": "./tools/executors/check-dependency-tree:check-dep-tree",
      "options": {
        "project": "storefront"
      }
    }
  }
}
